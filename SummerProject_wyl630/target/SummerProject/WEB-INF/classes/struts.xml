<?xml version="1.0" encoding="utf-8"?>
<!-- Struts2 DTD -->
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="utf-8" />
	<constant name="struts.devMode" value="true" />
	<package name="process" extends="struts-default">
		
		<action name="loginpage">
			<result name="success">/summerproject/jsp/NewFile.jsp</result>
		</action>
		
		<action name="login" class="action.login">

			<result name="success" type="redirect">/summerproject/jsp/GameMain.jsp</result>
			<result name="input">/summerproject/jsp/NewFile.jsp</result>
		</action>
		
		<action name="get_question" class="action.SendQuestion">
			<result name="success">/summerproject/game/index.html</result>
		</action>

		<action name="Gameover" class="action.GameOverAction">
			<result name="success" type="redirect">/summerproject/jsp/SumGame.jsp</result>
		</action>


		<action name="*">
			<result>/{1}.jsp</result>
		</action>
	</package>
</struts>